{
  "version": "1.0.44",
  "name": "chase-backend",
  "main": "src/main.ts",
  "scripts": {
    "dev": "bun run concurrently \"bun run dev:server\" \"bun run dev:docker\"",
    "dev:server": "bun run --watch --hot src/main.ts",
    "dev:docker": "docker compose up --remove-orphans",
    "build": "tsc --noEmit && bun build.ts"
  },
  "dependencies": {
    "@elysiajs/bearer": "^0.7.0",
    "@elysiajs/cors": "^0.7.0",
    "@elysiajs/eden": "^0.7.0",
    "@elysiajs/fn": "^0.6.1",
    "@elysiajs/swagger": "^0.7.0",
    "@prisma/client": "latest",
    "auth": "../../auth",
    "elysia": "^0.7.1"
  },
  "devDependencies": {
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "munify-util": "../../munify-util",
    "prisma": "^5.3.1",
    "@tsconfig/node16": "^16.1.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "module": "src/main.js"
}
