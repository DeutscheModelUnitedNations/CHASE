{
  "name": "chase-backend",
  "scripts": {
    "build": "tsc --noEmit && node build.js",
    "dev": "npm-run-all -p dev:server dev:db",
    "dev:server": "ts-node-dev src/main.ts -p tsconfig.json --respawn",
    "dev:db": "docker compose up",
    "dev:migrate": "npx prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@prisma/client": "^4.15.0",
    "auth": "file:../../auth",
    "fastify": "^4.17.0",
    "fastify-now": "^3.0.4",
    "fastify-type-provider-zod": "^1.1.9",
    "pino-pretty": "^10.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@fastify/swagger": "^8.4.0",
    "@fastify/swagger-ui": "^1.8.1",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.2.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.18.0",
    "esbuild-plugin-define": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3",
    "zod-prisma-types": "^2.7.4"
  }
}
