{
  "name": "chase",
  "scripts": {
    "dev": "./run_dev.sh",
    "dev:normal": "concurrently \"bun run dev:next\" \"bun run dev:docker\" \"bun run dev:prisma-studio\" \"bun run dev:gqty\" \"dev:i18n\"",
    "dev:container": "concurrently \"bun run dev:server\" \"bun run dev:prisma-studio\" \"bun run dev:gqty\" \"dev:i18n\"",
    "dev:next": "next dev",
    "dev:docker": "docker compose -f dev.docker-compose.yml up --remove-orphans",
    "dev:prisma-studio": "prisma studio --browser none",
    "dev:gqty": "bunx @gqty/cli --watch --react true --subscriptions graphql-ws --target ./src/app/util/gqty/gqty.ts --typescript true http://localhost:3000/api/graphql",
    "dev:i18n": "typesafe-i18n",
    "typecheck": "prisma generate && tsc --noEmit",
    "build": "next build",
    "start": "next start",
    "lint": "biome check . --apply"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.4.1",
    "@elysiajs/cors": "^1.0.2",
    "@gqty/react": "^3.0.1",
    "@pothos/core": "^3.41.1",
    "@pothos/plugin-complexity": "^3.13.0",
    "@pothos/plugin-prisma": "^3.65.2",
    "@pothos/plugin-prisma-utils": "^0.14.0",
    "@pothos/plugin-tracing": "^0.5.8",
    "@prisma/client": "^5.15.0",
    "@tailwindcss/typography": "^0.5.13",
    "elysia": "^1.0.23",
    "elysia-helmet": "^1.0.2",
    "framer-motion": "^11.2.10",
    "gqty": "^3.0.1",
    "graphql-ws": "^5.16.0",
    "graphql-yoga": "^5.3.1",
    "jose": "^5.4.0",
    "next": "14.2.4",
    "next-runtime-env": "^3.2.2",
    "oidc-client-ts": "^3.0.1",
    "openid-client": "^5.6.5",
    "primeicons": "^7.0.0",
    "primereact": "^10.6.6",
    "react": "^18.3.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-flip-move": "^3.0.5",
    "react-markdown": "^9.0.1",
    "react-oidc-context": "^3.1.0",
    "react-responsive": "^10.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.77.5",
    "typesafe-i18n": "^5.26.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@gqty/cli": "^4.0.1",
    "@types/bun": "^1.1.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "graphql": "^16.8.2",
    "postcss": "^8.4.38",
    "prisma": "^5.15.0",
    "prisma-generator-pothos-codegen": "^0.6.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "world-countries": "^5.0.0"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  }
}
